#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lmorrow/repo/v2/public/vite-project/node_modules/.pnpm/vite@5.4.15_@types+node@20.17.27/node_modules/vite/bin/node_modules:/Users/lmorrow/repo/v2/public/vite-project/node_modules/.pnpm/vite@5.4.15_@types+node@20.17.27/node_modules/vite/node_modules:/Users/lmorrow/repo/v2/public/vite-project/node_modules/.pnpm/vite@5.4.15_@types+node@20.17.27/node_modules:/Users/lmorrow/repo/v2/public/vite-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lmorrow/repo/v2/public/vite-project/node_modules/.pnpm/vite@5.4.15_@types+node@20.17.27/node_modules/vite/bin/node_modules:/Users/lmorrow/repo/v2/public/vite-project/node_modules/.pnpm/vite@5.4.15_@types+node@20.17.27/node_modules/vite/node_modules:/Users/lmorrow/repo/v2/public/vite-project/node_modules/.pnpm/vite@5.4.15_@types+node@20.17.27/node_modules:/Users/lmorrow/repo/v2/public/vite-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
